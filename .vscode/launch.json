  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch via NPM",
      "request": "launch",
      "runtimeArgs": ["run-script", "debug"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "command": "yarn stop;yarn start:dev",
      "name": "terminal debug",
      "request": "launch",
      "type": "node-terminal"
    },

    {
      "command": "yarn test",
      "name": "test debug",
      "request": "launch",
      "type": "node-terminal"
    },

    {
      "command": "yarn seed",
      "name": "seed",
      "request": "launch",
      "type": "node-terminal"
    },

    {
      "command": "yarn batch",
      "name": "test batch",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "yarn seed:iBTravelTag",
      "name": "test seed:iBTravelTag",
      "request": "launch",
      "type": "node-terminal"
    }
  ]
}
